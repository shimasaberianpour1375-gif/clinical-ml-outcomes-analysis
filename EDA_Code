    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "Ixzhd0O0mK1q"
      },
      "outputs": [],
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "\n",
        "# Load data\n",
        "df = pd.read_csv('physio.csv')\n",
        "\n",
        "# Target variable\n",
        "target = 'pain'\n",
        "\n",
        "# Categorical and numerical variables\n",
        "categorical_vars = ['gender', 'treatments', 'symptoms', 'episodes', 'baseline']\n",
        "numerical_vars = ['age']\n",
        "\n",
        "# -------------------------------\n",
        "# Overview and Summary Stats\n",
        "# -------------------------------\n",
        "print(\"\\n--- Dataset Overview ---\")\n",
        "print(df.head())\n",
        "\n",
        "print(\"\\n--- Data Info ---\")\n",
        "print(df.info())\n",
        "\n",
        "print(\"\\n--- Summary Statistics (Numerical) ---\")\n",
        "print(df[numerical_vars].describe())\n",
        "\n",
        "print(\"\\n--- Summary Statistics (Categorical) ---\")\n",
        "for col in categorical_vars:\n",
        "    print(f\"\\nValue counts for {col}:\")\n",
        "    print(df[col].value_counts())\n",
        "\n",
        "# Target variable distribution\n",
        "print(\"\\n--- Target Variable Distribution ---\")\n",
        "print(df[target].value_counts(normalize=True))\n",
        "\n",
        "# -------------------------------\n",
        "# Univariate Visualizations\n",
        "# -------------------------------\n",
        "sns.set(style=\"whitegrid\")\n",
        "\n",
        "# Dynamic subplot layout\n",
        "total_plots = 2 + len(categorical_vars)  # age + pain + 5 categoricals = 7\n",
        "cols = 2\n",
        "rows = (total_plots + 1) // cols\n",
        "\n",
        "fig, axes = plt.subplots(rows, cols, figsize=(14, rows * 4))\n",
        "axes = axes.flatten()\n",
        "\n",
        "# Age distribution\n",
        "sns.histplot(df['age'], bins=30, kde=True, ax=axes[0])\n",
        "axes[0].set_title(\"Age Distribution\")\n",
        "\n",
        "# Target distribution\n",
        "sns.countplot(x='pain', data=df, ax=axes[1])\n",
        "axes[1].set_title(\"Pain Frequency\")\n",
        "\n",
        "# Categorical variables\n",
        "for i, col in enumerate(categorical_vars, start=2):\n",
        "    sns.countplot(x=col, data=df, order=df[col].value_counts().index, ax=axes[i])\n",
        "    axes[i].set_title(f\"{col} Distribution\")\n",
        "    axes[i].tick_params(axis='x', rotation=30)\n",
        "\n",
        "# Hide unused subplots\n",
        "for j in range(total_plots, len(axes)):\n",
        "    fig.delaxes(axes[j])\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "# -------------------------------\n",
        "# Bivariate Visualizations\n",
        "# -------------------------------\n",
        "\n",
        "# Numerical vs Target (age vs pain)\n",
        "plt.figure(figsize=(8, 5))\n",
        "sns.boxplot(x='pain', y='age', data=df)\n",
        "plt.title(\"Age vs Pain\")\n",
        "plt.show()\n",
        "\n",
        "# Categorical vs Target\n",
        "for col in categorical_vars:\n",
        "    plt.figure(figsize=(6, 4))\n",
        "    sns.countplot(x=col, hue='pain', data=df, order=df[col].value_counts().index)\n",
        "    plt.title(f\"{col} vs Pain\")\n",
        "    plt.legend(title='Pain')\n",
        "    plt.xticks(rotation=30)\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "\n",
        "# -------------------------------\n",
        "# Cross-tabulations (categorical vs pain)\n",
        "# -------------------------------\n",
        "for col in categorical_vars:\n",
        "    print(f\"\\n--- Cross-tabulation: {col} vs Pain ---\")\n",
        "    ctab = pd.crosstab(df[col], df['pain'], normalize='index')\n",
        "    print(ctab)\n",
        "    ctab.plot(kind='bar', stacked=True, figsize=(6, 4), title=f'{col} vs Pain (proportion)')\n",
        "    plt.ylabel('Proportion')\n",
        "    plt.xticks(rotation=30)\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "\n",
        "# -------------------------------\n",
        "# Correlation Matrix (Numerical only)\n",
        "# -------------------------------\n",
        "print(\"\\n--- Correlation Matrix (Numerical + Target) ---\")\n",
        "corr_df = df[numerical_vars + [target]].copy()\n",
        "# Ensure target is numeric\n",
        "if corr_df[target].dtype == 'object':\n",
        "    corr_df[target] = corr_df[target].astype('category').cat.codes\n",
        "corr = corr_df.corr()\n",
        "\n",
        "plt.figure(figsize=(6, 4))\n",
        "sns.heatmap(corr, annot=True, cmap='coolwarm', fmt=\".2f\")\n",
        "plt.title(\"Correlation Matrix\")\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ]
    }
  ]
}